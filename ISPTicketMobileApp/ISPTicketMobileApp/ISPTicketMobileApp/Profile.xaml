<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ISPTicketMobileApp.Profile"
             Title="Profile"
             IconImageSource="icon_profile.png">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Profile" FontSize="24" HorizontalOptions="CenterAndExpand" />
            <Label Text="Your phone number:" />
            <Entry x:Name="txt_phone" IsReadOnly="True"  />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>